
#include <stdio.h>

// swapping the elements
void swap(int *xp, int *yp)
{
    int temp = *xp;
    *xp = *yp;
    *yp = temp;
}

class Solution
{
    public:
    //Function to sort the array using bubble sort-almost O(N)time complexity algorithm.
    void bubbleSort(int a[], int n)
    {
        // Your code here  
        for(int i=0;i<n-i-1;i++){
        int min_id=i,max_id=i;
        for(int j=i;j<n-i;j++){
            if(a[min_id]>a[j])
               min_id=j;
            if(a[max_id]<a[j])
               max_id=j;
        }
        //if(i!=min_id)
        swap((a+i),(a+min_id));
       if(i==max_id)
           max_id=min_id;
        swap((a+n-i-1),(a+max_id));
    }
    }
};


void printArray(int arr[], int size)
{
    int i;
    for (i=0; i < size; i++)
        printf("%d ", arr[i]);
    printf("\n");
}
 
int main()
{
    int arr[1000],n,T,i;
  
    scanf("%d",&T);
    
    while(T--){
        
    scanf("%d",&n);
    
    for(i=0;i<n;i++)
      scanf("%d",&arr[i]);
      
    Solution ob;  
      
    ob.bubbleSort(arr, n);
    printArray(arr, n);
    }
    return 0;;
} 
